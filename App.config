<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true" indentsize="0">
      <listeners>
        <add name="MyLog" type="System.Diagnostics.TextWriterTraceListener" initializeData="logfile.txt" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>